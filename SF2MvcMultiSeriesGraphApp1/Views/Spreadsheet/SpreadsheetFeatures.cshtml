@{
    ViewBag.Title = "Spreadsheet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.EJS().Spreadsheet("spreadsheet").Created("created").OpenUrl("Open").SaveUrl("Save").Sheets((sheet) =>
{
    sheet.Ranges((rangeSetting) =>
    {
        rangeSetting.DataSource(@ViewBag.defaultData).Add();
    }).Add();
}).Render()

<script>
    function created() {
        // Hiding the `Insert` from ribbon.
        this.hideRibbonTabs(['Insert']); }
</script>